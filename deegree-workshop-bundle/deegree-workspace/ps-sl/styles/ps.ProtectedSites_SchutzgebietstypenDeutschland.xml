    <sld:NamedLayer>
        <se:Name>PS.ProtectedSite.OSPAR</se:Name>
        <sld:UserStyle>
            <se:Name>inspire_common:PS.DE</se:Name>
            <se:FeatureTypeStyle>
				<se:FeatureTypeName>ps:ProtectedSite</se:FeatureTypeName>
                <se:Rule>
                    <se:Description>
                        <se:Title>OSPAR-Gebiet</se:Title>
                        <se:Abstract>
                    Gebiete nach dem Übereinkommen zum Schutz der Meeresumwelt des Nordost-Atlantiks (Oslo-Paris Konvention)
                        </se:Abstract>
                    </se:Description>
                    <ogc:Filter>                        <!-- Filterregel -->
                        <ogc:PropertyIsEqualTo>
                            <ogc:PropertyName>designation</ogc:PropertyName>
                            <ogc:Literal>http://registry.gdi-de.org/codelist/de.bund.bfn/SchutzgebietstypenDeutschland/OSPAR</ogc:Literal>
                        </ogc:PropertyIsEqualTo>
                    </ogc:Filter>
                    <se:PolygonSymbolizer>
                        <se:Fill>
                            <se:SvgParameter name="fill">#cc462c</se:SvgParameter>
                            <se:SvgParameter name="fill-opacity">1</se:SvgParameter>
                        </se:Fill>
                        <se:Stroke>
                            <se:SvgParameter name="stroke">#cc462c</se:SvgParameter>
                            <se:SvgParameter name="stroke-opacity">1</se:SvgParameter>
                            <se:SvgParameter name="stroke-width">1</se:SvgParameter>
                            <se:SvgParameter name="stroke-linejoin">round</se:SvgParameter>
                        </se:Stroke>
                    </se:PolygonSymbolizer>
                </se:Rule>
            </se:FeatureTypeStyle>
        </sld:UserStyle>
    </sld:NamedLayer>
